{
  "expo": {
    "name": "Hookd",
    "slug": "ObjectDetectionAppExpo",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your camera",
        "NSLocationWhenInUseUsageDescription": "We need your location to provide you with relevant nearby services."
      },
      "bundleIdentifier": "com.eddie.hooked"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./frame_0248.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eddie.hooked"
    },
    "web": {
      "favicon": "./frame_0248.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-video",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your camera"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c7aefda0-a249-43b1-900c-2a9178f6e486"
      }
    }
  }
}
